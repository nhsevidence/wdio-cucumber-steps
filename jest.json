{
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "collectCoverageFrom": [
      "src/support/**/*.js"
    ],
    "notify": true,
    "setupFilesAfterEnv": ["<rootDir>/test/setup.js"],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testURL": "http://localhost"
  }
